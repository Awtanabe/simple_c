<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @plan.name %>
</p>

<p>
  <strong>Price:</strong>
  <%= @plan.price %>
</p>
 
  <%= month_calendar events: @plan.schedules  do |date, meetings| %>

  <%= date %>

  <% meetings.each do |meeting| %>
 
    <div class="saasas">
      <%= meeting.start_time %>
    </div>
  <% end %>
<% end %>

<%= link_to 'Edit', edit_plan_path(@plan) %> |
<%= link_to 'Back', plans_path %>
